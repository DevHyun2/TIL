package com.shinhan.day01;

public class 예제62p {
	public static void main(String[] args) {
		byte result1 = 10 + 20;
		System.out.println("result1 : " + result1);
		
		byte v1 = 10;
		byte v2 = 100;
		int result2 = v1 + v2;
		System.out.println("result2 : " + result2);
		
		byte v3 = 10;
		int v4 = 100;
		long v5 = 1000L;
		long result3 = v3 + v4 + v5;
		System.out.println("result3 : " + result3);
		
	}

}
