package com.shinhan.day05;

public class CarTest {

	public static void main(String[] args) {
		method3();

	}

	private static void method3() {
		System.out.println("Car 대수: " + Car.carCount);
		System.out.println("Car company: " + Car.company);
		//1.객체사용시 객체 참조변수 선언 + 객체 생성 + 사용
		Car car1 = new Car("A모델", 1000);
		Car car2 = new Car("B모델", 2000);
		Car car3 = new Car("C모델", 3000);
		Car car4 = new Car("D모델");
		Car car5 = new Car(5000);
	}

	private static void method1() {
		//static은 class소유 이므로 object생성과 관계없다.
		System.out.println("Car 대수: " + Car.carCount);
		System.out.println("Car company: " + Car.company);
		Car.carInfoPrint2("*********");
		int a = 100;
		method2();
	}
	private static void method2() {
		System.out.println("------------");
	}
}
