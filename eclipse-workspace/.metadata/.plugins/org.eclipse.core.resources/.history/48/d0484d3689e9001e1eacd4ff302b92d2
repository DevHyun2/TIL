package train;

import java.util.Arrays;

public class 연습장 {

	public static void main(String[] args) {
		solution("ASDQ");
	}
	
	public static void solution(String my_string) {
		my_string = my_string.toLowerCase();
		
		char arr = my_string.toCharArray();
		Array.sort(arr);
		
		System.out.println(String(arr));
		
        
    }
}

