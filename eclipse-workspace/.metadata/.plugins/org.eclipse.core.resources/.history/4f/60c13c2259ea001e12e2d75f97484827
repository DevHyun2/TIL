package com.shinhan.week3.day10.제네릭;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@ToString
@Getter@Setter
@AllArgsConstructor
@NoArgsConstructor
public class Car {
	
	String model;
	int price;
	String color;

	public void run() {
		System.out.println("자동차가 달립니다.");
	}
	
	
}
