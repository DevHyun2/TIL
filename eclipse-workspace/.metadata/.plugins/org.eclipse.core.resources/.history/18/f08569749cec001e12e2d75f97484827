package com.shinhan.week3.day13;

import com.shinhan.week3.day12.Student;

interface Makealbe {
	Student makeStudent(String name, int score);
}

class StudentMakeClass implements Makealbe{
	public Student makeStudent(String name, int score) {
		return new Student(name, score);
	}

}

public class Review {
	public static void main(String[] args) {
		f1();
	}

	private static void f1() {
		Makealbe make1 = (n,s) -> new Student(n, s);
		Makealbe make2 = Student::new;
		Makealbe make3 = new Makealbe() {

			@Override
			public Student makeStudent(String name, int score) {
				return new Student(name, score);
			}};
		Makealbe make4 = new StudentMakeClass();
		
		
		
		Student s1 = make1.makeStudent("상호", 100);
		Student s2 = make2.makeStudent("상호2", 200);
		
		System.out.println(s1);
		System.out.println(s2);
	}
}
