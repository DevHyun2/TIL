package com.shinhan.week2.day06.lab;

class Tire {
	void run() {
		System.out.println("일반");
	}
}

class SnowTire extends Tire {
	@Override
	void run() {
		System.out.println("스노우");
	}
}

public class SnowTireExample {
	public static void main(String[] args) {

		SnowTire s = new SnowTire();
		Tire tire = s;

		s.run();
		tire.run();
	}
}