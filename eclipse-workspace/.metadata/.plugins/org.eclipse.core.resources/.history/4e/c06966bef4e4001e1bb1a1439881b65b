package com.shinhan.day05.test;

public class Account {
	String num;
	String name;
	int money;
	
	public Account(String num, String name, int money) {
		super();
		this.num = num;
		this.name = name;
		this.money = money;
	}
	
	void accPrint() {
		System.out.println("-----------------------------------");
		System.out.println("1.계좌생성|2.계좌목록|3.예금|4.출금|5.종료");
		System.out.println("-----------------------------------");
		
	}
}
