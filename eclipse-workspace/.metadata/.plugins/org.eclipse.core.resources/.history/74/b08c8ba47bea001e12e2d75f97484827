package com.shinhan.week3.day10.멀티스레드.practice;

public class Account {
	
	String accountNo; //계좌번호
	String ownerName; //예금주 이름
	int balance; //잔액
	
	//입금
	void deposit(int amount) {
		balance += amount;
	}
	
	//출금
	int withdraw(int amount) {
		if(balance >= amount) {
			return amount;
		}else {
			System.out.println("잔고부족");
		}return 0;
	}
	
	
}
