package com.shinhan.week2.day07;

public abstract class Shape {
	int numSides; //변의 수
	double area //넓이
	double permieter //둘레 길이
	
	Shape(int numSides){ 
		
	}

	public int getNumSides() {
		return numSides;
	}

	public abstract double getArea() {
		return area;
	}

	public abstract double getPermieter() {
		return permieter;
	}
	
	
}
