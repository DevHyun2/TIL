package com.shinhan.week3.day14;


public class Prob1 {
	public static void main(String[] args) {
		String[] array = { "황남기85점", "조성호89점", "한인성88점", "독고정진77점" };
		printMaxScore(array);
	}

	private static void printMaxScore(String[] array) {
		// 구현하세요.
		String str = "";
		String str1 = "";
		String str2 = "";
		String str3 = "";
		String str4 = "";
		
		for(int i=0 ; i<array.length ; i++) {
			if(array[i].length() == 6) {
				str += array[i].substring(3,5);
			}else {
				str += array[i].substring(4,6);
			}
		}
		str1 += str.substring(0,2);
		str2 += str.substring(2);
		str3 += str.substring(2);
		str4 += str.substring(2);
		System.out.println(str2);
		System.out.println("최고점수는 ");
	}
}