package com.shinhan.day04.LAB4;

public class Book {
	
	private String title;
	private int price;
	
	Book(String title, int price) {
		this.title = title;
		this.price = price;
	}
	
	//getter, setter
	public void setTitle(String title){
		this.title = title;
	}
	
	public String getTitle() {
		return title;
	}
	
	public void setPrice(int price) {
		this.price = price;
	}
	
	public String getPrice() {
		return price;
	}
	
	
	void bookInfo()	{
		
	}

}
