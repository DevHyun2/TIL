package com.shinhan.day04;

public class Review {

	public static void main(String[] args) {
		method1();

	}

	private static void method1() {
		//배열 : 같은 타입의 여러개의 값을 저장하기 위한 자료구조 
		//heap 영역에 연속공간을 만든다.
		//개수를 알아야 생성가능
		
		//1.배열선언
		int[] arr = null;
		
		//2.배열생성
		
		
		//3.배열사용
		arr[0] = 100;
		System.out.println(arr[0]);
	}

}
