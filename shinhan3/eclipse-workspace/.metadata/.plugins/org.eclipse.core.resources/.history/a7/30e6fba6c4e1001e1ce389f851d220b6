package com.shinhan.day03;

public class EnumTest {

	//상수 : 변하지 않는 값 - final static
	final static int SUCCES = 1;
	final static int FAIL = 0;
	
	public static void main(String[] args) {
		int score = 100;
		int result;
		
		if (score>=90) {
			result = SUCCES;
		}else {
			result = FAIL;
		}
		System.out.println(result);
	}

}
